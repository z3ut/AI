<UserControl x:Class="Inventor.Client.Controls.ProcessesStatementControl"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Vertical" Name="_contextControl">
		<GroupBox Name="_groupProcessA">
			<ComboBox Name="_comboBoxProcessA" SelectedItem="{Binding Path=ProcessA, Mode=TwoWay}" />
		</GroupBox>
		<GroupBox Name="_groupProcessB">
			<ComboBox Name="_comboBoxProcessB" SelectedItem="{Binding Path=ProcessB, Mode=TwoWay}" />
		</GroupBox>
		<GroupBox Name="_groupSequenceSign">
			<ComboBox Name="_comboBoxSequenceSign" SelectedItem="{Binding Path=SequenceSign, Mode=TwoWay}" />
		</GroupBox>
	</StackPanel>
</UserControl>
